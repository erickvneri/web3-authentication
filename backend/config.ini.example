[develop]
# App configs
app_name=localhost
host=127.0.0.1
port=5000
debug=True

# Moralis EVM configs
moralis_api_key=<MORALIS API KEY>
evm_domain=localhost
evm_url=http://localhost:5000/
evm_auth_timeout=15
evm_auth_chain_id=1
auth_expires_in=300

# JWT configs
jwt_secret=<JWT SECRET>
jwt_expires_in=3600
jwt_refresh_expires_in=604800
jwt_algorithm=HS256
